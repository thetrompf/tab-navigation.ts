{
    "extends": ["tslint-secoya"],
    "rules": {
        "indent": [true, "spaces", 4],
        "jsx-no-multiline-js": false,
        "no-relative-imports": false,
        "quotemark": [true, "single", "jsx-double"],
        "react-tsx-curly-spacing": false,
        "semicolon": [true, "always"]
    }
}

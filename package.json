{
    "name": "@secoya/tab-navigation.ts",
    "version": "1.0.0",
    "dependencies": {
        "@secoya/collection.ts": "^2.0.6",
        "@types/react-dom": "^15.5.1",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-dom": "^15.6.1"
    },
    "devDependencies": {
        "@types/jest": "^20.0.2",
        "@types/node": "^8.0.6",
        "@types/prop-types": "^15.5.1",
        "@types/react": "^15.0.33",
        "@types/rollup": "^0.41.1",
        "jest": "^20.0.4",
        "jest-environment-node-debug": "^2.0.0",
        "prettier": "^1.5.2",
        "rollup": "^0.43.0",
        "rollup-plugin-alias": "^1.3.1",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-ts": "^0.1.0",
        "ts-jest": "^20.0.6",
        "tslib": "^1.7.1",
        "tslint": "^5.4.3",
        "tslint-secoya": "^1.0.4",
        "typescript": "^2.4.1"
    },
    "jest": {
        "globals": {
            "__TS_CONFIG__": "tsconfig.test.json"
        },
        "moduleDirectories": [
            "node_modules"
        ],
        "modulePathIgnorePatterns": [
            "<rootDir>/build",
            "<rootDir>/.next"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx",
            "json"
        ],
        "testRegex": "/__tests__/.*\\.(ts|tsx)$",
        "transform": {
            "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        }
    },
    "license": "BSD-3-Clause",
    "main": "lib/index.js",
    "module": "lib2015/index.js",
    "repository": "https://github.com/secoya/form-tab-navigation.ts",
    "scripts": {
        "clean": "rm -Rf dist lib lib2015 libnext",
        "build": "tsc -p tsconfig.json && tsc -p tsconfig.es2015.json && tsc -p tsconfig.esnext.json && rollup -c && tsc -p tsconfig.system.json",
        "build:dist": "rm -Rf dist && rollup -c && tsc -p tsconfig.system.json",
        "build:lib-es5": "rm -Rf lib && tsc -p tsconfig.json",
        "build:lib-es2015": "rm -Rf lib2015 && tsc -p tsconfig.es2015.json",
        "build:lib-esnext": "rm -Rf libnext && tsc -p tsconfig.esnext.json",
        "fmt": "prettier --write --print-width 120 --tab-width 4 --single-quote --trailing-comma all --parser typescript \"src/**/*.ts?(x)\"",
        "lint": "tslint --project tsconfig.json",
        "prepublishOnly": "rm -Rf dist lib lib2015 libnext && tsc -p tsconfig.json && tsc -p tsconfig.es2015.json && tsc -p tsconfig.esnext.json && rollup -c && tsc -p tsconfig.system.json",
        "test": "jest"
    },
    "types": "lib/index.d.ts"
}

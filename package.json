{
    "name": "@secoya/tab-navigation.ts",
    "version": "1.1.1",
    "dependencies": {
        "@secoya/collection.ts": "^2.10.0",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-dom": "^15.6.1"
    },
    "devDependencies": {
        "@types/jest": "^20.0.4",
        "@types/node": "^8.0.14",
        "@types/prop-types": "^15.5.1",
        "@types/react": "^15.0.38",
        "@types/react-dom": "^15.5.1",
        "@types/rollup": "^0.41.1",
        "jest": "^20.0.4",
        "jest-environment-node-debug": "^2.0.0",
        "prettier": "^1.5.3",
        "rollup": "^0.45.2",
        "rollup-plugin-commonjs": "^8.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-replace": "^1.1.1",
        "rollup-plugin-ts": "^0.1.0",
        "ts-jest": "^20.0.7",
        "tslib": "^1.7.1",
        "tslint": "^5.5.0",
        "tslint-secoya": "^1.0.5",
        "typescript": "^2.4.1"
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "tsConfigFile": "tsconfig.test.json"
            }
        },
        "moduleDirectories": [
            "node_modules"
        ],
        "modulePathIgnorePatterns": [
            "<rootDir>/build",
            "<rootDir>/.next"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx",
            "json"
        ],
        "testRegex": "/__tests__/.*\\.(ts|tsx)$",
        "transform": {
            "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        }
    },
    "license": "BSD-3-Clause",
    "main": "lib/index.js",
    "module": "lib2015/index.js",
    "repository": "https://github.com/secoya/form-tab-navigation.ts",
    "scripts": {
        "clean": "rm -Rf dist lib lib2015 libnext types",
        "build": "tsc -p tsconfig.lib.json && tsc -p tsconfig.lib2015.json && tsc -p tsconfig.libnext.json && rollup -c rollup.config.dist.js",
        "build:dist": "rm -Rf dist && rollup -c rollup.config.dist.js",
        "build:example": "rollup -c rollup.config.example.js",
        "build:lib": "rm -Rf lib types && tsc -p tsconfig.lib.json",
        "build:lib2015": "rm -Rf lib2015 && tsc -p tsconfig.lib2015.json",
        "build:libnext": "rm -Rf libnext && tsc -p tsconfig.libnext.json",
        "fmt": "prettier --write --print-width 120 --tab-width 4 --single-quote --trailing-comma all --parser typescript \"src/**/*.ts?(x)\"",
        "lint": "tslint --project tsconfig.json",
        "prepublishOnly": "rm -Rf dist lib lib2015 libnext types && tslint --project tsconfig.json && jest && tsc -p tsconfig.lib.json && tsc -p tsconfig.lib2015.json && tsc -p tsconfig.libnext.json && rollup -c rollup.config.dist.js",
        "test": "jest"
    },
    "types": "lib/index.d.ts"
}

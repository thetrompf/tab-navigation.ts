    },
    "peerDependencies": {
        "react": ">= 15.6.2",
        "react-dom": ">= 15.6.2"
    },
    "devDependencies": {
        "@types/jest": "23.3.1",
        "@types/node": "10.7.1",
        "@types/prop-types": "15.5.5",
        "@types/react": "15.6.19",
        "@types/react-dom": "15.5.8",
        "jest": "23.5.0",
        "prettier": "1.14.2",
        "react": "15.6.2",
        "react-dom": "15.6.2",
        "react-testing-library": "5.0.0",
        "rollup": "0.64.1",
        "rollup-plugin-commonjs": "9.1.5",
        "rollup-plugin-node-resolve": "3.3.0",
        "rollup-plugin-replace": "2.0.0",
        "rollup-plugin-ts": "0.1.0",
        "ts-jest": "23.1.4",
        "tslib": "1.9.3",
        "tslint": "5.11.0",
        "tslint-secoya": "2.0.1",
